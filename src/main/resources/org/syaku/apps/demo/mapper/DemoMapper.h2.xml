<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.syaku.apps.mybatis.mapper.DemoMapper">

	<resultMap id="demo" type="org.syaku.apps.mybatis.domain.Demo">
		<id property="userId" column="user_id" />
		<result property="isUse" column="is_use" />
		<result property="userName" column="user_name" />
		<result property="userCount" column="user_count" />
	</resultMap>

	<select id="select" resultMap="demo">
		SELECT * FROM demo
	</select>

	<select id="selectOne" resultMap="demo">
		SELECT * FROM demo WHERE user_id = #{user_id} AND is_use = #{is_use}
	</select>

	<update id="insert">
		INSERT INTO demo (
			user_id,
			is_use,
			user_name,
			user_count
		) VALUES (
			#{userId},
			'Y',
			#{userName},
			#{userCount}
		)
	</update>

</mapper>